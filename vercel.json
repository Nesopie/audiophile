{
    "version": 2,
    "builds": [
      {
        "src": "./api/app.ts",  
        "use": "@vercel/node"
      },
      {
        "src": "./client/build",
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/app.ts"
      }
    ]
  }